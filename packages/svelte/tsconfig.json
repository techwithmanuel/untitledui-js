{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "types": ["svelte"],
    "paths": {
      "svelte": ["./node_modules/svelte"]
    }
  },
  "exclude": ["node_modules"]
}
